{
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png",
        "default_title": "MarketoLive"
    },
    "content_scripts": [ {
        "all_frames": false,
        "js": ["content.js"],
        "run_at": "document_end",
        "matches": [
            "*://*.marketo.com/*",
            "*://pages.marketolive.com/*",
            "*://dev.pages.marketolive.com/*"
        ]
    }, {
        "all_frames": false,
        "js": ["designer-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://*.marketodesigner.com/*"
        ]
    }, {
        "all_frames": false,
        "js": ["one-login-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://marketo.onelogin.com/client/apps"
        ]
    }, {
        "all_frames": false,
        "js": ["deliverability-tools-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://250ok.com/*"
        ]
    }, {
        "all_frames": false,
        "js": ["invision-app-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://marketo.invisionapp.com/*"
        ]
    }, {
        "all_frames": false,
        "js": ["google-search-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://www.google.com/search?dynamicAd=true&*"
        ]
    }, {
        "all_frames": false,
        "js": ["facebook-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://www.facebook.com/?dynamicAd=true&*"
        ]
    }, {
        "all_frames": false,
        "js": ["linkedin-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://www.linkedin.com/feed/?dynamicAd=true&*"
        ]
    } ],
    "externally_connectable": {
        "matches": [
            "*://marketolive.com/*",
            "*://www.marketolive.com/*",
            "*://dev.marketolive.com/*",
            "*://verticals.marketolive.com/*",
            "*://dev.verticals.marketolive.com/*",
            "*://pages.marketolive.com/*",
            "*://pages.dev.marketolive.com/*",
            "https://marketo.onelogin.com/client/apps",
            "https://app-sjdemo1.marketo.com/*",
            "https://app-sjp.marketo.com/*",
            "https://na-sjdemo1.marketodesigner.com/*",
            "https://na-sjp.marketodesigner.com/*",
            "https://sjrtp3.marketo.com/*",
            "https://sjrtp1.marketo.com/*",
            "https://sj-ee-api.marketo.com/*",
            "https://sjrcademo1.marketo.com/*",
            "https://sjrca02.marketo.com/*",
            "https://seo.marketo.com/*"
        ]
    },
    "icons": {
        "128": "images/icon.png",
        "16": "images/icon.png",
        "48": "images/icon.png"
    },
    "permissions": [
        "management", "storage", "webRequest", "webRequestBlocking", "webNavigation", "tabs", "cookies", "activeTab", "notifications", "browsingData",
        "*://*.marketo.com/*",
        "*://*.marketodesigner.com/*",
        "*://pages.marketolive.com/*",
        "*://dev.pages.marketolive.com/*",
        "*://marketolive.com/*",
        "*://www.marketolive.com/*",
        "*://dev.marketolive.com/*",
        "*://verticals.marketolive.com/*",
        "*://dev.verticals.marketolive.com/*",
        "*://250ok.com/*",
        "*://marketo.invisionapp.com/*",
        "*://marketo.onelogin.com/*",
        "*://www.google.com/search?dynamicAd=true&*",
        "*://www.facebook.com/?dynamicAd=true&*",
        "*://www.linkedin.com/feed/?dynamicAd=true&*"
    ],
    "content_security_policy": "script-src 'self' https://marketolive.com https://cdn.heapanalytics.com/js/heap-702959232.js https://cdn.heapanalytics.com/js/heap-2823746907.js; object-src 'self'",
    "web_accessible_resources": ["lib/*"],
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "author": "Marketo Demo Services",
    "name": "MarketoLive Dev",
    "description": "This is the development version of the MarketoLive extension and is used exclusively for testing purposes.",
    "version": "5.1.1",
    "key": ""
}