{
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png",
        "default_title": "MarketoLive"
    },
    "content_scripts": [ {
        "all_frames": false,
        "js": ["content.js"],
        "run_at": "document_end",
        "matches": [
            "*://*.marketo.com/*",
            "*://*.marketodesigner.com/*",
            "*://pages.marketolive.com/*",
            "*://pages-dev.marketolive.com/*"
        ]
    }, {
        "all_frames": false,
        "js": ["one-login-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://marketo.onelogin.com/client/apps"
        ]
    }, {
        "all_frames": false,
        "js": ["deliverability-tools-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://250ok.com/*"
        ]
    }, {
        "all_frames": false,
        "js": ["invision-app-content.js"],
        "run_at": "document_end",
        "matches": [
            "*://marketo.invisionapp.com/*"
        ]
    } ],
    "externally_connectable": {
        "matches": [
            "*://marketolive.com/*",
            "*://www.marketolive.com/*",
            "*://www.marketolive-dev.com/*",
            "*://verticals.marketolive.com/*",
            "*://verticals-dev.marketolive.com/*",
            "https://marketo.onelogin.com/client/apps",
            "https://app-sjdemo1.marketo.com/*",
            "https://app-sjp.marketo.com/*"
        ]
    },
    "icons": {
        "128": "images/icon.png",
        "16": "images/icon.png",
        "48": "images/icon.png"
    },
    "permissions": [
        "management", "storage", "webRequest", "webRequestBlocking", "webNavigation", "tabs", "cookies", "activeTab", "notifications",
        "*://*.marketo.com/*",
        "*://*.marketodesigner.com/*",
        "*://pages.marketolive.com/*",
        "*://pages-dev.marketolive.com/*",
        "*://marketolive.com/*",
        "*://www.marketolive.com/*",
        "*://www.marketolive-dev.com/*",
        "*://verticals.marketolive.com/*",
        "*://verticals-dev.marketolive.com/*",
        "*://250ok.com/*",
        "*://marketo.invisionapp.com/*",
        "*://marketo.onelogin.com/*"
    ],
    "content_security_policy": "script-src 'self' https://marketolive.com https://cdn.heapanalytics.com/js/heap-702959232.js https://cdn.heapanalytics.com/js/heap-2823746907.js; object-src 'self'",
    "web_accessible_resources": ["lib/*"],
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "author": "Marketo Demo Services",
    "name": "MarketoLive",
    "description": "This extension is for Marketo employees and partners who have access to the MarketoLive demo environment.",
    "version": "5.0.2"
}