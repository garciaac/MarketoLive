{
    "background": {
        "persistent": true,
        "scripts": ["background.min.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png",
        "default_title": "MarketoLive"
    },
    "content_scripts": [{
        "all_frames": false,
        "js": ["content.min.js"],
        "run_at": "document_start",
        "matches": [
            "*://*.marketo.com/*",
            "*://*.marketodesigner.com/*",
            "*://marketolive.com/*",
            "*://www.marketolive.com/*",
            "*://cloud4.insightera.com/*",
            "*://go.app.io/*",
            "https://250ok.com/*",
            "https://marketo.invisionapp.com/*",
            "https://marketo.onelogin.com/*"
        ]
    }],
    "externally_connectable": {
        "matches": [
            "*://marketolive.com/*",
            "https://marketo.onelogin.com/client/apps"
        ]
    },
    "icons": {
        "128": "images/icon.png",
        "16": "images/icon.png",
        "48": "images/icon.png"
    },
    "permissions": [
        "management", "storage", "webRequest", "webNavigation", "tabs", "cookies", "activeTab",
        "*://*.marketo.com/*",
        "*://*.marketodesigner.com/*",
        "*://marketolive.com/*",
        "*://www.marketolive.com/*",
        "*://cloud4.insightera.com/*",
        "*://go.app.io/*",
        "https://250ok.com/*",
        "https://marketo.invisionapp.com/*",
        "https://marketo.onelogin.com/*"
    ],
    "content_security_policy": "script-src 'self' https://marketolive.com; object-src 'self'",
    "web_accessible_resources": ["lib/*"],
    "manifest_version": 2,
    "update_url": "https://clients2.google.com/service/update2/crx",
    "name": "MarketoLive",
    "description": "This plugin is for Marketo employees who have access to the MarketoLive demo environment.",
    "version": "4.1.2"
}